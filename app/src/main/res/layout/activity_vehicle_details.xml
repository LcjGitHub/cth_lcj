<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@color/f9"
    tools:context=".activity.VehicleDetailsActivity">
    <com.chetuhui.lcj.chezhubao_x.view.titlebar.CommonTitleBar
        android:id="@+id/titlebar_vd"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:centerText="车辆详情"
        app:centerType="textView"
        app:fillStatusBar="false"
        app:leftImageResource="@drawable/back"
        app:leftType="imageButton"
        app:rightText=""
        app:rightType="textView"
        app:showBottomLine="false"
        app:titleBarColor="@color/transparent" />
    <LinearLayout
        android:layout_below="@+id/titlebar_vd"
        android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text="车牌号"/>
            <TextView
                android:id="@+id/tv_vd_cph"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>
        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text="所有人"/>
            <TextView
                android:id="@+id/tv_vd_syr"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>

        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text="品牌型号"/>
            <TextView
                android:id="@+id/tv_vd_ppxh"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>
        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text="车辆识别代号"/>
            <TextView
                android:id="@+id/tv_vd_clsbdh"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>
        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text="发动机型号"/>
            <TextView
                android:id="@+id/tv_vd_fdjxh"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>
        <RelativeLayout
            android:layout_marginBottom="@dimen/dp_2"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:layout_centerVertical="true"
                android:text="行驶证照片"/>
            <ImageView
                android:id="@+id/iv_vd_xszzp"
                android:layout_alignParentRight="true"
                android:layout_width="@dimen/dp_125"
                android:layout_height="@dimen/dp_85"
                android:textSize="@dimen/dp_15"
                android:textColor="@color/_3"
                android:layout_margin="@dimen/dp_15"
                android:text=""/>


        </RelativeLayout>


    </LinearLayout>


</RelativeLayout>